piepline {
    agent any 
    stages {
        stage('STAGE-1') {
            when {
                branch 'main'
            }
            steps{
                sh "echo STEGE-2 executes if branch is main"
            }
        }

        stage('STAGE-2'){
            when {
                branch 'master'
            }
            steps{
                sh "echo STEGE-2 executes if branch is master"
            }
        }
    }
}
